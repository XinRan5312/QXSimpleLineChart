<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>
    <style name="aty_invested_detail_current_text">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/colorPrimary</item>
        <item name="android:textSize">12dp</item>
        <item name="android:gravity">center</item>
    </style>
    <style name="aty_invested_detail_current_text_container">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">vertical</item>
        <item name="android:layout_weight">1</item>
    </style>
    <style name="WarpLinearLayoutDefault">
        <item name="grivate">left</item>
        <item name="horizontal_Space">20dp</item>
        <item name="vertical_Space">20dp</item>
        <item name="isFull">false</item>
    </style>
    <style name="act_feedback_tab_btn_style_four">
        <item name="android:background">@drawable/selector_tag_btn</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">40dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_marginLeft">16dp</item>
        <item name="android:paddingLeft">18dp</item>
        <item name="android:button">@null</item>
    </style>
    <style name="act_feedback_tab_btn_style_three">
        <item name="android:background">@drawable/selector_tag_btn</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">40dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_marginLeft">16dp</item>
        <item name="android:paddingLeft">20dp</item>
        <item name="android:button">@null</item>
    </style>
    <style name="act_feedback_tab_btn_style_two">
        <item name="android:background">@drawable/selector_tag_btn</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">40dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_marginLeft">16dp</item>
        <item name="android:paddingLeft">30dp</item>
        <item name="android:button">@null</item>
    </style>

</resources>
